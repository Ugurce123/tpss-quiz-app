{
  "buildCommand": "cd client && rm -rf node_modules && npm install && npm run build",
  "outputDirectory": "client/build",
  "installCommand": "npm install",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "api/**"
    }
  }
}
